{
  "Cors": {
    "AllowedOrigins": [
      "https://admin.domainname.com"
    ],
    "AllowCredentials": true
  },
  "AuthTokenOptions": {
    "AccessTokenTtlMinutes": 15,
    "RefreshTokenTtlDays": 30,
    "RefreshTokenPepper": "",
    "UseRefreshTokenCookie": true,
    "RefreshCookieName": "rt",
    "RefreshCookieSameSite": "Lax",
    "RefreshCookiePath": "/auth/refresh"
  }
}
